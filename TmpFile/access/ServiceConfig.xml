<!--
    XXX.XXX.XXX.XXX     IceGrid Master节点的IP
    xxxx                IceGrid Master节点的端口
    YYY.YYY.YYY.YYY     IceGrid Replica节点的IP
    yyyy                IceGrid Replica节点的端口
    access_instance_id  Access Service实例ID
    access_instance_passwd  Access Service实例密码

    AAA.AAA.AAA.AAA     Access Service实例的IP
    aaaa                Access Service实例的端口
    bbbb                HTTP2 Service实例的端口
--><Root>
    <NATIPList>
        <NATIP/>
    </NATIPList>

    <!--日志保存天数-->
    <LogSaveDays>15</LogSaveDays>

    <ICEServer>
        <!--对应IceGrid MasterRegistry-->
        <DBServer>192.168.7.105</DBServer>
        <DBPort>10000</DBPort>
    </ICEServer>
    <ICEServer>
        <!--对应IceGrid BackupRegistry-->
        <DBServer>192.168.7.107</DBServer>
        <DBPort>10001</DBPort>
    </ICEServer>

    <ProcessList>
        <ServiceProcess>
            <ProcessName>service</ProcessName>
            <ListenList>
                <Listen>
                    <Addr>0</Addr>
                    <Protocol>TCP</Protocol>
                    <Port>1089</Port>
                </Listen>
                <Listen>
                    <Addr>0</Addr>
                    <Protocol>UDP</Protocol>
                    <Port>1089</Port>
                </Listen>
            </ListenList>

            <Service>
                <Guid>{DB6C80F7-ECCA-474E-84C9-E62CC6DF61A3}</Guid>
                <Name>access_service</Name>
                <Priority>3</Priority>
                <ApplicationID>101</ApplicationID>
                <Config>
                    <ManagerServiceIPv4Addr>TCP:192.168.7.106:1089</ManagerServiceIPv4Addr>
                    <ServiceInstanceID>access_instance_1</ServiceInstanceID>
                    <ServiceInstancePassword>access_instance_fsp</ServiceInstancePassword>
                </Config>
            </Service>

            <Service>
                <Guid>{71cfc567-7bb5-f57a-1a89-45e7247fa0c2}</Guid>
                <Name>http2_service</Name>
                <Priority>2</Priority>
                <ApplicationID>103</ApplicationID>
                <Config>
                    <ListenAddress>0</ListenAddress>
                    <ListenPort>3000</ListenPort>
                    <CrtFile>keys/server.crt</CrtFile>
                    <KeyFile>keys/server.2048.key</KeyFile>
                </Config>
            </Service>

        </ServiceProcess>
    </ProcessList>
</Root>